<div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header"><%= t("activerecord.models.user_type", count: @user_types.count) %></h1>
                </div>
              </div>
    <table class="table table-condensed">
      <thead>
        <td>
          <%= link_to new_user_type_path, class: "btn btn-success" do %>
          <i class="fa fa-plus"></i>
          <%= t(:new) %>
          <% end %>
        </td>
      </thead>
    </table>
    <div class="table-responsive">

    <div id="userTypes_details">
    <%= render 'userTypes' %>
</div>
</div>
<div id="paginator" align='center'>
    <div>
      <%= paginate @user_types, :remote=>true %>
    </div>
</div>
</div>




